<p id="notice"><%= notice %></p>

<h1>Listing Addresses</h1>

<table>
  <thead>
    <tr>
      <th>Vendor</th>
      <th>Type</th>
      <th>Lat</th>
      <th>Lng</th>
      <th>Wikipedia</th>
      <th>Iata</th>
      <th>Code</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @addresses.each do |address| %>
      <tr>
        <td><%= address.vendor_id %></td>
        <td><%= address._type %></td>
        <td><%= address.lat %></td>
        <td><%= address.lng %></td>
        <td><%= address.wikipedia %></td>
        <td><%= address.iata %></td>
        <td><%= address.code %></td>
        <td><%= link_to 'Show', address %></td>
        <td><%= link_to 'Edit', edit_address_path(address) %></td>
        <td><%= link_to 'Destroy', address, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Address', new_address_path %>
